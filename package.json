{
  "name": "streamer_raider",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run front/watch\" \"npm run server/watch\"",
    "build": "npm run front/build && npm run server/build",
    "front/serve": "vue-cli-service serve",
    "front/watch": "npm run front/serve",
    "front/build": "vue-cli-service build",
    "server/watch": "tsc -w -p ./tsconfig_server.json",
    "server/build": "tsc -p ./tsconfig_server.json",
    "server/start": "pm2 start bootstrap-pm2.json"
  },
  "dependencies": {
    "concurrently": "^6.2.1",
    "connect-history-api-fallback": "^1.6.0",
    "crypto-js": "^4.1.1",
    "discord.js": "^13.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "express-slow-down": "^1.4.0",
    "multiparty": "^4.2.2",
    "node-fetch": "^2.6.1",
    "tmi.js": "^1.8.5",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/gsap": "^1.20.2",
    "@types/multiparty": "0.0.33",
    "@types/node-fetch": "^2.5.12",
    "@types/tmi.js": "^1.8.1",
    "@types/uuid": "^8.3.1",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "gsap": "^3.7.1",
    "less-loader": "^7.0.1",
    "vue-headful": "^2.1.0",
    "vue-i18n": "^8.25.0",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
